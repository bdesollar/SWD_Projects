<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="615.0" prefWidth="891.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RootFinderGuiController">
   <children>
      <Label fx:id="numberOfSetsLabel" layoutX="28.0" layoutY="61.0" text="Please Enter Number of Sets to Calculate" />
      <TextField fx:id="numberOfSetsTextField" layoutX="316.0" layoutY="56.0" />
      <Button layoutX="523.0" layoutY="56.0" mnemonicParsing="false" onAction="#buttonPressed" text="Enter" />
      <LineChart fx:id="lineChart" layoutX="28.0" layoutY="148.0" title="Performance of Threads">
        <xAxis>
          <NumberAxis autoRanging="false" label="Number of Calculations Performed" lowerBound="250" side="BOTTOM" tickUnit="10.0" upperBound="350.0" />
        </xAxis>
        <yAxis>
          <NumberAxis label="Time Taken to Perform Calculations" side="LEFT" />
        </yAxis>
      </LineChart>
      <TextArea fx:id="outputDataTextArea" layoutX="613.0" layoutY="47.0" prefHeight="247.0" prefWidth="255.0" />
   </children>
</AnchorPane>
